<Window x:Class="WpfApplication2.FIXWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApplication2"
        mc:Ignorable="d"
        Title="Dodaj do zamówienia" Height="165.046" Width="300">
    <Grid Margin="0,0,2,-5">
        <ComboBox Name="Books" ItemsSource="{Binding booklist}"
                  Margin="27,10,132,98"/>
        <TextBlock Text="Książka" Margin="174,17,10,45"/>
        <TextBox Name="Amt" Margin="27,54,132,55" TextChanged="Amt_TextChanged"/>
        <TextBlock Text="Ilość" Margin="174,62,34,10"/>
        <Button Name="Addbtn" Content="Dodaj" Click="Addbtn_Click" Margin="5,97,168,10" />
        <Button Name="Cancel" Content="Anuluj" Click="Cancel_Click" Margin="163,97,10,10" />
    </Grid>
</Window>
